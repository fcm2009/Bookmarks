@MISC{Denier2006UnderstandingDPDensity,
  author = {Simon Denier and Pierre Cointe},
  title = {Understanding Design Patterns Density with Aspects: {A} Case Study
	in {JH}otDraw using Aspect{J}},
  year = {2006},
  abstract = {Design patterns offer solutions to common engineering prob- lems in
	programs [1]. In particular, they shape the evolution of program
	elements. However, their implementations tend to vanish in the code:
	thus it is hard to spot them and to understand their impact. The
	prob- lem becomes even more difficult with a {"}high density of pattern{"}:
	then the program becomes easy to evolve in the direction allowed
	by patterns but hard to change [2]. Aspect languages offer new means
	to modular- ize elements. Implementations of object-oriented design
	patterns with AspectJ have been proposed [3]. We aim at testing the
	scalability of such solutions in the JHotDraw framework. We first
	explore the impact of density on pattern implementation. We show
	how AspectJ helps to reduce this impact. This unveils the principles
	of aspects and AspectJ to control pattern density.},
  bibsource = {OAI-PMH server at hal.archives-ouvertes.fr},
  comment = {hal:http://hal.archives-ouvertes.fr/inria-00458193/en/; http://hal.archives-ouvertes.fr/docs/00/45/81/93/PDF/denier-sc06.pdf},
