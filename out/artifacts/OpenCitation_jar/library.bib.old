@MISC{zhao02metricSuiteAOP,
  author = {Jianjun Zhao},
  title = {Towards {A} Metrics Suite for Aspect-Oriented Software},
  month = jul # {~11},
  year = {2002},
  abstract = {Although a large body of research in software metrics has been focused
	on procedural or object-oriented software, there is no software metric
	for aspect-oriented software until now. In this paper, we propose
	some metrics for aspect-oriented software, which are specically designed
	to quantify the information ows in an aspect-oriented program. We
	define these metrics based on a dependence model for aspect-oriented
	software which consists of a group of dependence graphs each can
	be used to explicitly represent various dependence relations at dierent
	levels of an aspect-oriented program. The proposed metrics can be
	used to measure the complexity of an aspect-oriented program from
	various dierent viewpoints.},
  annote = {Jianjun Zhao (Department of Computer Science and Engineering; Fukuoka
	Institute of Technology; 3-30-1 Wajiro-Higashi , Higashi-ku , Fukuoka
	811-0295 , Japan);},
